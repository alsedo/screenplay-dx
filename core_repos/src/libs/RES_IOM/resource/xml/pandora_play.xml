		<image x="844" image="@@bg-CMD3"/>
		<image x="911" image="@@bg-CMD4"/>
		<image x="978" image="@@bg-CMD5"/>
		<image x="1045" image="@@bg-CMD6"/>
	</wrapper>

	<text text="@@text-buttons" disable="@@disable-text-buttons" x="643" y="422" w="370" h="20" fontsize="18" textcolor="0x0" align="hcenter" />
	<image x="643" y="344" w="64" h="64" image="@@thumb_up"/>
	<image x="710" y="344" w="64" h="64" image="@@thumb_down"/>
	<include filename="./inc_pandora_loading.xml"/>
	<include filename="./inc_decoration_online.xml"/>
</page>
