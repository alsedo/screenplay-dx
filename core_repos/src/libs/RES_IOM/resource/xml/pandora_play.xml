	<image image="image/pandora/PANDORA-Station-cover_play_17.png" x="569" y="227" w="49" h="43"/>
	<text text="@@songname" x="639" y="234" w="434" h="34" fontsize="24" numlines="1" textcolor="0x0064aa" speed="1" delay="2" align="left"/>
	<image x="636" y="273" w="434" h="12" image="image/pandora/PANDORA-Station-cover_play_line.png" bg="1"/>

	<image image="@@play_state" x="569" y="291" w="48" h="48"/>
	<progress_bar name="point_bar" x="638" y="305" w="432" h="21" pb_back_img="image/pandora/PANDORA-Station-cover_play_n.png" pb_front_img="image/pandora/PANDORA-Station-cover_play_f.png" pb_delay="500"/>
	<text x="1080" y="304" w="69" h="22" fontsize="20" text="@@audio_currenttime" align="left" textcolor="0x0"/>
	
	<image x="213" y="505" w="41" h="37" image="image/pandora/PANDORA-Station-cover_play_Artist.png"/>
