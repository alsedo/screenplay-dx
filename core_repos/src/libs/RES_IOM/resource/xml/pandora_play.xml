<page background="image/pandora/pandora_bg.png">
	<ctrlgrp valkey="options" autoenter="1" lbmask="LR"> 
			<button valkey="CMD0"
					focused_bg="image/pandora/PANDORA-Station-cover_play_I-like-this-song-f.png"
		    	    normal_bg="image/pandora/PANDORA-Station-cover_play_I-like-this-song-n.png"
	            	group_valkey="buttons"
			        group_focused_text="I like this song"/>
	        <button valkey="CMD1"
            		focused_bg="image/pandora/PANDORA-Station-cover_play_I-don’t-like-this-song-f.png"
			        normal_bg="image/pandora/PANDORA-Station-cover_play_I-don’t-like-this-song-n.png"
		    	    group_valkey="buttons"
		        	group_focused_text="I don't like this song"/>
        	<button valkey="CMD2"
			        focused_bg="image/pandora/PANDORA-Station-cover_play_Bookmark-f.png"
			        normal_bg="image/pandora/PANDORA-Station-cover_play_Bookmark-n.png"
		    	    group_valkey="buttons"
		        	group_focused_text="Bookmark"/>
			<button valkey="CMD3"
            		focused_bg="image/pandora/PANDORA-Station-cover_play_I-am-tired-of-this-song-f.png"
            		normal_bg="image/pandora/PANDORA-Station-cover_play_I-am-tired-of-this-song-n.png"
            		group_valkey="buttons"
            		group_focused_text="I'm tired of this song"/>
	        <button valkey="CMD4"
            		focused_bg="image/pandora/PANDORA-Station-cover_play_Why-play-the-f.png"
            		normal_bg="image/pandora/PANDORA-Station-cover_play_Why-play-the-n.png"
            		group_valkey="buttons"
		       	 	group_focused_text="Why was this song selected?"/>
	        <button valkey="CMD5"
            		focused_bg="image/pandora/PANDORA-Station-cover_play_Rename-station-f.png"
            		normal_bg="image/pandora/PANDORA-Station-cover_play_Rename-station-n.png"
            		group_valkey="buttons"
		       	 	group_focused_text="Rename station"/>
	        <button valkey="CMD6"
            		focused_bg="image/pandora/PANDORA-Station-cover_play_1_Delete-station-f.png"
            		normal_bg="image/pandora/PANDORA-Station-cover_play_1_Delete-station-n.png"
            		group_valkey="buttons"
		       	 	group_focused_text="Delete station"/>

	</ctrlgrp>

	<image x="179" y="173" w="370" h="324" image="image/pandora/PANDORA-Station-cover_play_Box.png" bg="1"/>
