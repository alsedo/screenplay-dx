	<image x="213" y="546" w="41" h="37" image="image/pandora/PANDORA-Station-cover_play_Album.png"/>
	<text text="@@album" x="262" y="548" w="715" h="30" fontsize="24" align="left" speed="1" delay="2" textcolor="0x0"/>

	<image x="213" y="587" w="41" h="37" image="image/pandora/PANDORA-Station-cover_play_Station-name.png"/>
	<text x="262" y="590" w="715" h="30" text="@@station" fontsize="24" textcolor="0x0" speed="1" delay="2" align="left"/>

	<!-- fixed -->

	<wrapper y="344" w="67" h="72">
		<image x="643" image="@@bg-CMD0"/>
		<image x="710" image="@@bg-CMD1"/>
