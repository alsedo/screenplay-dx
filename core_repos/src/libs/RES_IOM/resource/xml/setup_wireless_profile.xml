    <image lightbar="@@lightbar-profile_list-4" active="@@act-profile_list-4"/>
 </table>

  <table x="250" y="188" w="330" h="245" textcolor="0xffffff" actnoseltextcolor="0x00ccff" fontsize="22" selected_fontsize="30" cols="1" rows="5" border="0" speed="1" delay="2">
	  <text text="@@title-profile_list-0" active="@@act-profile_list-0"/>
	  <text text="@@title-profile_list-1" active="@@act-profile_list-1"/>
	  <text text="@@title-profile_list-2" active="@@act-profile_list-2"/>
	  <text text="@@title-profile_list-3" active="@@act-profile_list-3"/>
	  <text text="@@title-profile_list-4" active="@@act-profile_list-4"/>
  </table>

  <table x="600" y="188" w="372" h="245" textcolor="0xffffff" actnoseltextcolor="0x00ccff" fontsize="22" selected_fontsize="30" cols="1" rows="5" border="0" speed="1" delay="2">
	  <text text="@@ss-profile-0" active="@@act-profile_list-0"/>
	  <text text="@@ss-profile-1" active="@@act-profile_list-1"/>
