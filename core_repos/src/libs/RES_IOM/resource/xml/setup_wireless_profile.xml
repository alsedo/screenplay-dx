    <button valkey="DONE" focused_bg="image/network_Wireless_browse_favorites_btn_f.png" normal_bg="image/network_Wireless_browse_favorites_btn_n.png"/>
  </ctrlgrp>

  <include filename="./inc_setup_entry_basic_title.xml"/>

  <table x="185" y="188" w="913" h="245" cols="1" rows="5" lbimage="./image/settings_focus_btn.png">
