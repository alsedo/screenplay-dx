<?xml version="1.0"?>
<page background="./image/browse_bg.jpg">
  <ctrlgrp>
    <listbox valkey="profile_list" autoenter="1" move="row" cols="1" rows="5" lbmask="l" loop_scroll="1"/>
    <linebreak/>
    <button valkey="CONNECT" focused_bg="image/network_Wireless_browse_favorites_btn_f.png" normal_bg="image/network_Wireless_browse_favorites_btn_n.png" disabled_bg="image/network_Wireless_browse_favorites_btn_n.png"/>
    <button valkey="FAVORITE" focused_bg="image/network_Wireless_browse_favorites_btn_f.png" normal_bg="image/network_Wireless_browse_favorites_btn_n.png" disabled_bg="image/network_Wireless_browse_favorites_btn_n.png"/>
