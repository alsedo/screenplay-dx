  <imgbtn image="@@bg-CONNECT" text="$$Connect now" align="hcenter" x="181" y="516" w="220" h="42" textcolor="0xffffff" actnoseltextcolor="0xffffff" fontsize="15" selected_fontsize="18" active="@@act-CONNECT"/>
