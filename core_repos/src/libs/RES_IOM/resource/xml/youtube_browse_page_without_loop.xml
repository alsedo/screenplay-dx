<?xml version="1.0"?>
<page background="./image/youtube/youtube_bg.jpg">
	<ctrlgrp lbmask="LR">
    		<listbox valkey="browselist" autoenter="1" move="page" cols="1" rows="5" lbmask="lrud"  loop_scroll="0" disable_light_bar_click="1"/>
		<listbox valkey="browse_page_options" move="row" autoenter="1" cols="1" rows="8" lbmask="lr" disable_light_bar_click="1"/>
	</ctrlgrp>

	<image image="image/youtube/youtube_logo.png" x="129" y="74" w="150" h="75"/>
  
	<table x="365" y="166" w="728" h="450" fontsize="18" selected_fontsize="18"
		cols="1" rows="5" colspacing="0" rowspacing="0"
		lbimage="image/youtube/youtube_browser_Select_frame_f.png" clickimage=""  
		textcolor="0xb8b8b8" actnoseltextcolor="0xb8b8b8" actoverlistcolor="0xb8b8b8" 
		activetextcolor="0xb8b8b8" selectedtextcolor="0xb8b8b8" align="top">
		<text text="@@browse_0_views" lmargin="419" ymargin="55"
			lightbar="@@lightbar-browselist-0" active="@@act-browselist"
			selected="@@browse_0_select" disable="@@disable-browselist"/>
		<text text="@@browse_1_views" lmargin="419" ymargin="55"
			lightbar="@@lightbar-browselist-1" active="@@act-browselist" 
			selected="@@browse_1_select" disable="@@disable-browselist"/>
		<text text="@@browse_2_views" lmargin="419" ymargin="55" 
			lightbar="@@lightbar-browselist-2" active="@@act-browselist" 
			selected="@@browse_2_select" disable="@@disable-browselist"/>
		<text text="@@browse_3_views" lmargin="419" ymargin="55" 
			lightbar="@@lightbar-browselist-3" active="@@act-browselist" 
			selected="@@browse_3_select" disable="@@disable-browselist"/>
		<text text="@@browse_4_views" lmargin="419" ymargin="55" 
			lightbar="@@lightbar-browselist-4" active="@@act-browselist" 
			selected="@@browse_4_select" disable="@@disable-browselist"/>
	</table>
 
	<wrapper x="509" w="568" h="26" fontsize="22" textcolor="0xffffff"> 
		<text text="@@browselist_0_text" y="185" speed="1" delay="2"/>	
		<text text="@@browselist_1_text" y="275" speed="1" delay="2"/>	
		<text text="@@browselist_2_text" y="365" speed="1" delay="2"/>	
		<text text="@@browselist_3_text" y="455" speed="1" delay="2"/>	
		<text text="@@browselist_4_text" y="545" speed="1" delay="2"/>	
	</wrapper>

	<wrapper x="675" w="80" h="20" align="top" fontsize="18" textcolor="0xb8b8b8">
		<text text="@@browse_0_duration" y="221" />
		<text text="@@browse_1_duration" y="311" />
		<text text="@@browse_2_duration" y="401" />
		<text text="@@browse_3_duration" y="491" />
		<text text="@@browse_4_duration" y="581" />
	</wrapper>

	<!-- Stars -->
	<wrapper x="509" w="142" h="28">
		<image image="@@browse_0_stars" y="217"/>
		<image image="@@browse_1_stars" y="307"/>
		<image image="@@browse_2_stars" y="397"/>
		<image image="@@browse_3_stars" y="487"/>
		<image image="@@browse_4_stars" y="577"/>
	</wrapper>

	<wrapper x="375" w="120" h="68" align="hcenter" scale="100">
		<image image="@@browse_0_icon" ymargin="0" y="177"/>
		<image image="@@browse_1_icon" ymargin="0" y="267"/>
		<image image="@@browse_2_icon" ymargin="0" y="357"/>
		<image image="@@browse_3_icon" ymargin="0" y="447"/>
		<image image="@@browse_4_icon" ymargin="0" y="537"/>
  	</wrapper>

	<image image="@@moreprev" x="1111" y="173" w="39" h="22"/>
	<image image="@@morenext" x="1111" y="595" w="39" h="22" />

	<table x="122" y="175" w="232" h="344"
    		cols="1" rows="8" colspacing="0" rowspacing="0"
    		lbimage="image/youtube/youtube_browser_btn.png"
    		fontsize="22" selected_fontsize="22"
   		textcolor="0xffffff" align="left">
		<text text="@@options_0_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-0" active="@@act-browse_page_options"
			speed="1" delay="2"/>
		<text text="@@options_1_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-1" active="@@act-browse_page_options"
			speed="1" delay="2"/>
		<text text="@@options_2_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-2" active="@@act-browse_page_options"
			speed="1" delay="2"/>
		<text text="@@options_3_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-3" active="@@act-browse_page_options"
			speed="1" delay="2"/>
		<text text="@@options_4_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-4" active="@@act-browse_page_options"
			speed="1" delay="2"/>
		<text text="@@options_5_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-5" active="@@act-browse_page_options"
			speed="1" delay="2"/>   
		<text text="@@options_6_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-6" active="@@act-browse_page_options"
			speed="1" delay="2"/>   
		<text text="@@options_7_text" lmargin="20" ymargin="8"
			lightbar="@@lightbar-browse_page_options-7" active="@@act-browse_page_options"
			speed="1" delay="2"/>
	</table>

	<wrapper x="129" w="218" h="1">
    		<image image="@@options_0_line" ymargin="0" y="217"/>
		<image image="@@options_1_line" ymargin="0" y="260"/>
    		<image image="@@options_2_line" ymargin="0" y="303"/>
		<image image="@@options_3_line" ymargin="0" y="346"/>
    		<image image="@@options_4_line" ymargin="0" y="389"/>
	    	<image image="@@options_5_line" ymargin="0" y="432"/>
    		<image image="@@options_6_line" ymargin="0" y="475"/>
	    	<image image="@@options_7_line" ymargin="0" y="518"/>
	</wrapper>

	<text text="@@pagecounter" x="279" y="137" h="26" w="692" fontsize="24" align="right" textcolor="0xffffff"/>
	
	<include filename="./inc_loading.xml"/> 
	<include filename="./inc_decoration_online.xml"/>
</page>
