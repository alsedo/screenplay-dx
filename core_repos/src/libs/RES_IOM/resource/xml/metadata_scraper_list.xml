<?xml version="1.0"?>
<page>
	<ctrlgrp>
                <listbox valkey="browselist" autoenter="1" move="row" cols="1" rows="6" loop_scroll="0" lbmask="r"/>
		<linebreak/>
		<button valkey="CANCEL" focused_bg="image/global_cancel_bt_f.png" normal_bg="image/global_cancel_bt_n.png" click_bg="image/global_cancel_bt_n.png"/>
		<button valkey="OK" focused_bg="image/global_ok_bt_f.png" normal_bg="image/global_ok_bt_n.png" click_bg="image/global_ok_bt_n.png"/>
        </ctrlgrp>

        <image image="image/global_messagebox_large_bg.png" x="226" y="108" w="832" h="515"/>

        <text text="@@title" x="254" y="139" w="778" h="38" fontsize="36" align="left"/>
        <table x="266" y="207" w="749" h="360"  selected_fontsize="30" 
                        textcolor="0xc1c1c1" actnoseltextcolor="0xffffff" actoverlistcolor="0xc1c1c1" 
                        activetextcolor="0xff8400" selectedtextcolor="0xff8400"
                        cols="1" rows="6" colspacing="0" rowspacing="0" ymargin="0"
                        lbimage="image/global_messagebox_large_select_bar.png" fontsize="30"
                        lightbar_hide_if_unfocused="0">
                <text text="@@browse_0_name" lmargin="20" rmargin="47" speed="@@browse_0_speed" delay="2" 
                                lightbar="@@lightbar-browselist-0" active="@@act-browselist-0" selected="@@browse_0_select"/>
                <text text="@@browse_1_name" lmargin="20" rmargin="47" speed="@@browse_1_speed" delay="2" 
                                lightbar="@@lightbar-browselist-1" active="@@act-browselist-1" selected="@@browse_1_select"/>
                <text text="@@browse_2_name" lmargin="20" rmargin="47" speed="@@browse_2_speed" delay="2" 
                                lightbar="@@lightbar-browselist-2" active="@@act-browselist-2" selected="@@browse_2_select"/>
                <text text="@@browse_3_name" lmargin="20" rmargin="47" speed="@@browse_3_speed" delay="2" 
                                lightbar="@@lightbar-browselist-3" active="@@act-browselist-3" selected="@@browse_3_select"/>
                <text text="@@browse_4_name" lmargin="20" rmargin="47" speed="@@browse_4_speed" delay="2" 
                                lightbar="@@lightbar-browselist-4" active="@@act-browselist-4" selected="@@browse_4_select"/>
                <text text="@@browse_5_name" lmargin="20" rmargin="47" speed="@@browse_5_speed" delay="2" 
                                lightbar="@@lightbar-browselist-5" active="@@act-browselist-5" selected="@@browse_5_select"/>
        </table>

        <image image="@@thumbnail" x="0" y="0" w="172" h="120" default_image="@@default_thumbnail_listpage" scale="100" scale_type="fixratio" attr_num="6" attr_cur="@@thumb_active" disable="1">
                <attr x_offset="832" y_offset="265" disable="0" align="right,top"/>
                <attr x_offset="832" y_offset="325" disable="0" align="right,top"/>
                <attr x_offset="832" y_offset="385" disable="0" align="right,top"/>
                <attr x_offset="832" y_offset="269" disable="0" align="right,bottom"/>
                <attr x_offset="832" y_offset="329" disable="0" align="right,bottom"/>
                <attr x_offset="832" y_offset="389" disable="0" align="right,bottom"/>
        </image>

        <image image="@@moreprev" x="593" y="570" w="37" h="24"/>
        <image image="@@morenext" x="654" y="570" w="37" h="24"/>

	<imgbtn x="1096" y="303" w="116" h="116" image="@@bg-CANCEL" align="hcenter" active="@@act-CANCEL" disable="@@disable-CANCEL"/>
	<imgbtn x="1096" y="303" w="116" h="116" image="@@bg-OK" align="hcenter" active="@@act-OK" disable="@@disable-OK"/>
</page>

