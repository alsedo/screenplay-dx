mainmenu "DMAX Configuration"

config	VERSION
	string "version"
	default "0.00.01"	

menu "Preparation"
	

	menu "Product selection"
		choice
			prompt	"Product extension"
			config	PRODUCT_EXT_IOM
				bool "Iomega"
		endchoice
		choice
			prompt "Iomgea product extension"
			depends on PRODUCT_EXT_IOM
			config  PRODUCT_EXT_IOM_T4
				bool "T4 with Externel HDD"
			config  PRODUCT_EXT_IOM_X2
				bool "X2 with Internel HDD"
		endchoice

		config	CUSTOMER_NAME
			string	"Defines the customer name of this product"
			default	"IOMEGA" if PRODUCT_EXT_IOM
			default "GENERIC" if PRODUCT_EXT_ALPHA

		config	MODEL_NAME
			string	"Defines the model type of customer"
			default	"IOM-554" if PRODUCT_EXT_IOM_T4
			default	"IOM-554H" if PRODUCT_EXT_IOM_X2

		config PRODUCT_SN
			string
			default "PRODUCT_SN"
		
		choice 
			prompt "UI logic"
			config UI_LOGIC_IOM
				bool "IOM"
				depends on (PRODUCT_EXT_IOM)
		endchoice
		choice
			prompt	"Basic Pages Selection"
			config	BASIC_PAGES_GENERIC
				bool "Pages"
				help 
				"original Pages Folder, for general used"
		endchoice
		config UI_LOGIC
			string
			default "IOM_pages" if UI_LOGIC_IOM
		
		choice
			prompt	"IOM UI OEM"
				depends on PRODUCT_EXT_IOM
			config OEM_IOM
				bool "IOM with alpha UI"
		endchoice

		choice
			prompt "Resource"
			config RESOURCE_IOM
				bool "IOM Common UI"
				depends on OEM_IOM
		endchoice
		config RESOURCE
			string
			default "RES_IOM" if RESOURCE_IOM

		config SUPPORT_EXTRA_RESOURCE
			bool "EXTRA option"
			default n

		config EXTRA_RESOURCE
			string
			depends on SUPPORT_EXTRA_RESOURCE

		choice
			prompt "Model (World wide, US, EU, ....)"
			config MODEL_WW
				bool "World wide"
			config MODEL_US
				bool "United states only"
			config MODEL_EU
				bool "Europe"
		endchoice
		config MODEL
			string
			default "ww" if MODEL_WW
			default "us" if MODEL_US
			default "eu" if MODEL_EU

		config TVDOCK
			bool "TVDOCK (Should be removed)"
			default y
		config PRODUCT
			string "Product define (Should be removed)"
			default "TVDOCK"
		config CONFIG_HEADER_TYPE
			string "config file header type"
			default "A"
			help
				config_tool use this option to identify config file
		config CONFIG_HEADER_VERSION
			string "config file header version"
			default "1"
			help
				config_tool use this option to identify config file

	endmenu

	menu "Platform selection"
		choice
			prompt	"Platform"
			config	PLATFORM_SMP8654
				bool "Sigma Designs SMP8654"
				depends on PRODUCT_EXT_IOM 
		endchoice
		choice
			prompt	"Board selection"
			config	PLATFORM_SMP8654_ALPHA_IOM_BOARD
				bool "SMP8654 Iomega by Alpha"
				depends on PLATFORM_SMP8654
				help
					RAM(MB) : Bank0 256, Bank1 256
		endchoice
		config PLATFORM_SMP8654_WISTRON_BOARD_FIX_REBOOT
			bool "Patch fix for reboot"
			default n
			help
				Workaround for old wistron boards which doesn't have watchdog so that we have to reboot through GPIO

		menu "Common compile options"
			config COMPILE_WITH_GFLAG
				string "Compile with debug information (-g or -ggdb)"
				default "-ggdb" if  PLATFORM_X86
				default ""      if !PLATFORM_X86
			config COMPILE_WITH_OPTIMIZE
				string "Optimize flag (-O, ....)"
				default ""      if  PLATFORM_X86
				default "-O3"    if !PLATFORM_X86
		endmenu
		
		menu "SMP8634 compile options"
			depends on PLATFORM_SMP8634
			choice 
				prompt "SDK"
				config PLATFORM_SMP8634_SDK_2_8_3_0
					bool "2.8.3.0"
				config PLATFORM_SMP8634_SDK_2_8_4_1
					bool "2.8.4.1"
			endchoice
			choice
				prompt "toolchain"
				config PLATFORM_SMP8634_TOOLCHAIN_2_8_2_0
					bool "2.8.2.0"
			endchoice
		endmenu

		menu "SMP8654 compile options"
			depends on PLATFORM_SMP8654 
			choice 
				prompt "SDK"
				config PLATFORM_SMP8654_SDK_3_5_0
					bool "3.5.0"
				config PLATFORM_SMP8654_SDK_3_6_0_rc11
					bool "3.6.0_rc11"
				config PLATFORM_SMP8654_SDK_3_6_0
					bool "3.6.0"
				config PLATFORM_SMP8654_SDK_3_6_1_rc2
					bool "3.6.1_rc2"
				config PLATFORM_SMP8654_SDK_3_7_0
					bool "3.7.0"
				config PLATFORM_SMP8654_SDK_3_8_0
					bool "3.8.0"
				config PLATFORM_SMP8654_SDK_3_8_1
					bool "3.8.1"
				config PLATFORM_SMP8654_SDK_3_8_2
					bool "3.8.2"
				config PLATFORM_SMP8654_SDK_3_8_3
					bool "3.8.3"
				config PLATFORM_SMP8654_SDK_3_8_5
					bool "3.8.5"
				config PLATFORM_SMP8654_SDK_3_8_7
					bool "3.8.7"
				config PLATFORM_SMP8654_SDK_3_8_8
					bool "3.8.8"
			endchoice
			choice
				depends on PLATFORM_SMP8654_SDK_3_8_8 || PLATFORM_SMP8670_SDK_3_8_8
				prompt  "SDK Sub Version"
				config  PLATFORM_SMP8654_SDK_SUBVERSION_rc_3
					bool    "rc_3"
				config  PLATFORM_SMP8654_SDK_SUBVERSION_rc_6
					bool    "rc_6"
				config  PLATFORM_SMP8654_SDK_SUBVERSION_rc_11
					bool    "rc_11"
				config  PLATFORM_SMP8654_SDK_SUBVERSION_rc_15
					bool    "rc_15"
				config  PLATFORM_SMP8654_SDK_SUBVERSION_rc_16
					bool    "rc_16"

			endchoice
			config SUPPORT_DCCHD
				bool "Using DCCHD"
				default y
				depends on PLATFORM_SMP8654_SDK_3_8_0 || PLATFORM_SMP8654_SDK_3_8_1 || PLATFORM_SMP8654_SDK_3_8_2 || PLATFORM_SMP8654_SDK_3_8_3 || PLATFORM_SMP8654_SDK_3_8_5 || PLATFORM_SMP8654_SDK_3_8_7 || PLATFORM_SMP8654_SDK_3_8_8
				help
					This feature will compile DCCHD, you can use it to play DVD or UI. 
			config SUPPORT_DCCHD_DVD_BD_NAVIGATION
				bool "Using DCCHD to do DVD/BD navigation"
				default y
				depends on PLATFORM_SMP8654 && SUPPORT_DCCHD
				help
					This feature will enable full DVD and AVCHD navigation. The dmaosd will call DCCHD to play DVD or AVCHD. 
			choice
				prompt "DCCHD version"
				depends on SUPPORT_DCCHD_DVD_BD_NAVIGATION
				config 3_8_0_AQUA
					bool "3_8_0_aqua" 
				config 3_8_1_RC1_AQUA
					bool "3_8_1_rc1_aqua" 
				config 3_8_2_RC1_AQUA
					bool "3_8_2_rc1_aqua" 
				config 3_8_2_AQUA
					bool "3_8_2_aqua" 
				config 3_8_3_RC1_AQUA
					bool "3_8_3_rc1_aqua" 
				config 3_8_3_AQUA
					bool "3_8_3_aqua" 
				config 3_8_5_RC2_AQUA
					bool "3_8_5_rc2_aqua" 
				config 3_8_8_AQUA
					bool "3_8_8_aqua"
				config 3_8_8_RC6_AQUA
					bool "3_8_8_rc6_aqua"	
				config 3_8_8_RC11_AQUA
					bool "3_8_8_rc11_aqua"	
					depends on PLATFORM_SMP8654_SDK_3_8_8
				config 3_8_8_RC15_AQUA
					bool "3_8_8_rc15_aqua"	
					depends on PLATFORM_SMP8654_SDK_3_8_8
				config 3_8_8_RC16_AQUA
					bool "3_8_8_rc16_aqua"	
					depends on PLATFORM_SMP8654_SDK_3_8_8
			endchoice
			config DCCHD_VERSION
				string
				depends on SUPPORT_DCCHD
				default "SMP8652_3_8_0_aqua.mips" if 3_8_0_AQUA
				default "SMP8652_3_8_1_rc_1_aqua.mips" if 3_8_1_RC1_AQUA
				default "SMP8652_3_8_2_rc_1_aqua.mips" if 3_8_2_RC1_AQUA
				default "SMP8654_3_8_2_aqua.mips" if 3_8_2_AQUA
				default "SMP8654_3_8_3_rc_1_aqua.mips" if 3_8_3_RC1_AQUA
				default "SMP8652_3_8_3_aqua.mips" if 3_8_3_AQUA
				default "SMP8652_3_8_5_rc_2_aqua.mips" if 3_8_5_RC2_AQUA
				default "SMP8652_3_8_8_rc_3_aqua.mips" if 3_8_8_AQUA
				default "SMP8654_3_8_8_rc_6_aqua.mips" if 3_8_8_RC6_AQUA
				default "SMP8652_3_8_8_rc_11_aqua.mips" if 3_8_8_RC11_AQUA
				default "SMP8652_3_8_8_rc_15_aqua.mips" if 3_8_8_RC15_AQUA
				default "SMP8652_3_8_8_rc_16_aqua.mips" if 3_8_8_RC16_AQUA
			config DCCHD_MMAP_CONFIG_FILE
				string
				default "dcchd_config_3_8_0.h" if 3_8_0_AQUA || 3_8_1_RC1_AQUA || 3_8_2_RC1_AQUA
                        config DIRECTFBRC_BUILDER
                                bool "create directfbrc by securesoho config"
                                depends on SUPPORT_DCCHD
                                default y
                                help
                                        Using securesoho's config to create  directfbrc file for DCCHD.

			config USE_DIRECTFB_PORTING_SOURCECODE
				bool "Use DirectFB porting source code"
				depends on SUPPORT_DCCHD && (3_8_2_AQUA || 3_8_3_RC1_AQUA || 3_8_3_AQUA || 3_8_5_RC2_AQUA || 3_8_8_AQUA || 3_8_8_RC6_AQUA || 3_8_8_RC11_AQUA || 3_8_8_RC15_AQUA || 3_8_8_RC16_AQUA)
				default n
			config DIRECTFB_PORTING_VERSION
				depends on USE_DIRECTFB_PORTING_SOURCECODE
				string 
				default "SMP8654_3_8_2_porting.mips" if 3_8_2_AQUA
				default "SMP8654_3_8_3_rc_1_porting.mips" if 3_8_3_RC1_AQUA
				default "SMP8654_3_8_3_porting.mips" if 3_8_3_AQUA
				default "SMP8654_3_8_5_rc_2_porting.mips" if 3_8_5_RC2_AQUA
				default "SMP8654_3_8_8_rc_3_porting.mips" if 3_8_8_AQUA
				default "SMP8654_3_8_8_rc_6_porting.mips" if 3_8_8_RC6_AQUA
				default "SMP8654_3_8_8_rc_11_porting.mips" if 3_8_8_RC11_AQUA
				default "SMP8654_3_8_8_rc_15_porting.mips" if 3_8_8_RC15_AQUA
				default "SMP8654_3_8_8_rc_16_porting.mips" if 3_8_8_RC16_AQUA
                        config MMAP_DOUBLE_TO_SINGLE
				bool "MMAP_DOUBLE_TO_SINGLE"
				depends on 3_8_8_RC6_AQUA
				default y
				help
					the default setting is DRAM1(DCCHD), we need to change to DRAM0
					SMP8654 DRAM1(linux kernel) / DRAM0 (for rua memory, DCCHD need more memory space)
			config DIRECTFB_OVERWRITE_SIMGA_PNG_PATCH
				bool "Overwrite sigma png patch"
				depends on USE_DIRECTFB_PORTING_SOURCECODE && (3_8_3_AQUA ||3_8_5_RC2_AQUA || 3_8_8_AQUA || 3_8_8_RC6_AQUA || 3_8_8_RC11_AQUA || 3_8_8_RC15_AQUA || 3_8_8_RC16_AQUA)
				default y
				help
					Use original png provider code to overwrite sigma patch because it will crash randomly.
			config DIRECTFB_HDMI_FIX_COLOR_DEPTH_SWITCHING
				bool "HDMI Fix 12bit color depth switching patch"
				depends on USE_DIRECTFB_PORTING_SOURCECODE && 3_8_3_AQUA
				default y
				help
					We have to hotplug HDMI cable to make color depth ative.
					Root cause is DFB didn't call hdmi_video_conversion_colordepth().
					This patch is to fix DFB didn't update hdmi_video_conversion_colordepth(). 
			config DCCHD_FIX_HDMI_NOT_SYNC
				bool "Fix HDMI no output after removed power cable over 5 minutes"
				depends on USE_DIRECTFB_PORTING_SOURCECODE && (3_8_5_RC2_AQUA || 3_8_7_AQUA || 3_8_8_AQUA || 3_8_8_RC6_AQUA)
				default y
				help
					[HDMI] DDC fail if user boot up Villa after remove power cable over 5 minutes
			config DIRECTFB_HDMI_FIX_1080P24_SWITCHING
				bool "HDMI Fix fail to switch from 1080p24 to Auto"
				depends on USE_DIRECTFB_PORTING_SOURCECODE && (3_8_3_AQUA || 3_8_8_RC11_AQUA || 3_8_8_RC15_AQUA || 3_8_8_RC16_AQUA)
				default y
				help
					Fix can't change resolution from Auto to 1080p24.
			config DIRECTFB_HDMI_FIX
				bool "HDMI Fix patch"
				depends on USE_DIRECTFB_PORTING_SOURCECODE && 3_8_2_AQUA
				default n
				help
					The DFB HDMI thread fails if you connect HDMI to TV and change TV sources. The patch is from Yanquan of Sigma. We are runing Netflix project. 
			config DIRECTFB_FIX_COLOR_SPACE
				bool "fix dcchd can't set color space"
				depends on USE_DIRECTFB_PORTING_SOURCECODE
				default n
				help
					DCCHD doesn't use the color space that we setup.
					It use preferences color space.
					This patch will skip preferences color space if we had selected color space.
			config DIRECTFB_FIX_CRASH_IN_INVALID_ISO
				bool "fix dcchd crash in reading invalid iso file"
				depends on USE_DIRECTFB_PORTING_SOURCECODE
				default y
				help
					DCCHD crash if the iso is incorrectly DVD/AVCHD rip file.
					Skip this iso if it's incorrectly rip file.
			config DCCHD_FIX_HDMI_SPD_WORDS
				bool "fix HDMI SPD name to HD Media Player"
				depends on USE_DIRECTFB_PORTING_SOURCECODE
				default y
				help
					fix ITR No.: 18893HDMI: During HDMI handshaking, Bungalow is recognized by TV as "Bluray SMP_8654"
					change the SPD to "HD Media Player"	
			config DCCHD_FIX_USB_HOTPLUG_CRASH
				bool "fix dcchd dvd navi. crash if the current dvd file be removed"
				depends on 3_8_3_RC1_AQUA || 3_8_3_AQUA || 3_8_5_RC2_AQUA || 3_8_8_AQUA
				default y
				help
					fix system reboot if unplug USB disk during dvd playback.
					sigma fix this, but we have to replace librmdvdfs.so.
			choice
				prompt "select original patch for your kernel version"
				config PLATFORM_SMP8654_SDK_3_7_0_ORIGINAL_KERNEL_PATCH
					bool "using 3_7_0_KERNEL"
				config PLATFORM_SMP8654_SDK_3_8_2_ORIGINAL_KERNEL_PATCH
					bool "using 3_8_2_KERNEL"
				config PLATFORM_SMP8654_SDK_3_8_7_ORIGINAL_KERNEL_PATCH
					bool "using 3_8_7_KERNEL"
				help
					The original LINUX Kernel needs sigma's patch for SMP865x. 
					You have to select one for your kernel.
			endchoice
		
			config SUPPORT_DRIVE_LARGER_THAN_2T
				bool 
				depends on PLATFORM_SMP8654_SDK_3_7_0_ORIGINAL_KERNEL_PATCH
				default y if PLATFORM_SMP8654_SDK_3_7_0_ORIGINAL_KERNEL_PATCH

			config PLATFORM_SMP8654_SDK_3_7_1_RC3_BASE_ON_3_7_0
				bool "3.7.1_rc3 SDK base on 3.7.0"
				depends on PLATFORM_SMP8654_SDK_3_7_0
				default n
				help
					we just use 3.7.1_rc3 mrua and mruafw SDK. 
					the player code is the same the 3.7.0

			choice
				prompt "toolchain"
				config PLATFORM_SMP8654_TOOLCHAIN_3_4_0
					bool "3.4.0"
				config PLATFORM_SMP8654_TOOLCHAIN_3_6_0
					bool "3.6.0"
				config PLATFORM_SMP8654_TOOLCHAIN_GNU_4_3_2
					bool "mips4.3.2"
			endchoice
			
                        choice
                                prompt "Choose ezboot version" 
                        	depends on PLATFORM_SMP8654
			        config SIGMA_EZBOOT_0X82_NAND_FLASH_DRIVER
                                        bool "Use ezboot_0x82 to generate bootloader"
			        config SIGMA_EZBOOT_0X98_NAND_FLASH_DRIVER
                                        bool "Use ezboot_0x98 to generate bootloader for New NAND flash driver"
			        config SIGMA_EZBOOT_0XAA_NAND_FLASH_DRIVER
                                        bool "Use ezboot_0xaa to generate bootloader for New NAND flash driver"
                        endchoice

			choice
				prompt "Choose yamon version"
				depends on PLATFORM_SMP8654
				config SIGMA_YAMON_R2_13_17_NAND_FLASH_DRIVER
                                	bool "Use yamon_R2.13-17"
				config SIGMA_YAMON_R2_13_24_NAND_FLASH_DRIVER
                                	bool "Use yamon_R2.13-24 for New NAND flash driver"
				config SIGMA_YAMON_R2_13_31_NAND_FLASH_DRIVER
                                	bool "Use yamon_R2.13-31 for New NAND flash driver"
			endchoice

			choice
				prompt "Choose kernel proprietary"
				depends on PLATFORM_SMP8654
                        	config LINUX_KERNEL_2_6_22_19_19_NAND_FLASH_DRIVER
                                	bool "Proprietary from Linux_kernel_2.6.22.19-19 package"
                        	config LINUX_KERNEL_2_6_22_19_27_NAND_FLASH_DRIVER
                                	bool "Proprietary_27 from Linux_kernel_2.6.22.19-27 package for New NAND flash drive"
                        	config LINUX_KERNEL_2_6_22_19_35_NAND_FLASH_DRIVER
                                	bool "Proprietary_35 from Linux_kernel_2.6.22.19-35 package for New NAND flash drive"
                        	config LINUX_KERNEL_2_6_22_19_36_NAND_FLASH_DRIVER
                                	bool "Proprietary_36 from Linux_kernel_2.6.22.19-36 package for New NAND flash drive"
			endchoice

                        config DTS_CHIP_SET
                                bool "DTS_chip_set"
                                default n
                                help
                                     DTS chip set,We change the sigma SDK (mruafw) for DTS.You can see scripts/target-smp8654.mk.

                        config HAVE_VIDEO_CODEC_RMVB
                                bool "Use SMP86xx AU chip set"
                                default n
                                help
                                     AU chip set which support video codec rmvb.

			config PLATFORM_SMP8654_TOOLCHAIN_3_6_0_CACHE
				bool "Enable toolchain compile cache (it seems to related nand flash write)"
				depends on PLATFORM_SMP8654_TOOLCHAIN_3_6_0
				default y 

			config TOOLCHAIN_FILENAME
				string
				default "toolchain_smp86xx_3.4.0.tgz" if PLATFORM_SMP8654_TOOLCHAIN_3_4_0
				default "smp86xx_toolchain_R3.6_nofpu.tgz" if PLATFORM_SMP8654_TOOLCHAIN_3_6_0 && PLATFORM_SMP8654_TOOLCHAIN_3_6_0_CACHE
				default "toolchain_smp86xx_3.6.0.tgz" if PLATFORM_SMP8654_TOOLCHAIN_3_6_0 && ! PLATFORM_SMP8654_TOOLCHAIN_3_6_0_CACHE
				default "mips-4.3.tgz" if PLATFORM_SMP8654_TOOLCHAIN_GNU_4_3_2
			config CROSS_BIN
				string
				default "/opt/smp86xx_toolchain_3.4.0/bin/" if PLATFORM_SMP8654_TOOLCHAIN_3_4_0
				default "/opt/smp86xx_toolchain_R3.6_nofpu/build_mipsel_nofpu/staging_dir/bin/" if PLATFORM_SMP8654_TOOLCHAIN_3_6_0 && PLATFORM_SMP8654_TOOLCHAIN_3_6_0_CACHE
				default "/opt/smp86xx_toolchain_R3.6/bin/" if PLATFORM_SMP8654_TOOLCHAIN_3_6_0 && ! PLATFORM_SMP8654_TOOLCHAIN_3_6_0_CACHE
				default "/opt/mips-4.3/bin/" if PLATFORM_SMP8654_TOOLCHAIN_GNU_4_3_2
			config CROSS
				string
				default "$(CROSS_BIN)/mipsel-linux-"
			config CROSS_PREFIX
				string
				default "/opt/smp86xx_toolchain_3.4.0/" if PLATFORM_SMP8654_TOOLCHAIN_3_4_0
				default "/opt/smp86xx_toolchain_R3.6_nofpu/build_mipsel_nofpu/staging_dir/" if PLATFORM_SMP8654_TOOLCHAIN_3_6_0 && PLATFORM_SMP8654_TOOLCHAIN_3_6_0_CACHE
				default "/opt/smp86xx_toolchain_R3.6/" if PLATFORM_SMP8654_TOOLCHAIN_3_6_0 && ! PLATFORM_SMP8654_TOOLCHAIN_3_6_0_CACHE
				default "/opt/mips-4.3/" if PLATFORM_SMP8654_TOOLCHAIN_GNU_4_3_2
		endmenu

		config SIGMA_SDK_VERSION
			string
			default "2.8.3.0" if PLATFORM_SMP8634_SDK_2_8_3_0 
			default "2.8.4.1" if PLATFORM_SMP8634_SDK_2_8_4_1 
			default "3_5_0" if PLATFORM_SMP8654_SDK_3_5_0 
			default "3_6_0_rc11" if PLATFORM_SMP8654_SDK_3_6_0_rc11 
			default "3_6_0" if PLATFORM_SMP8654_SDK_3_6_0 
			default "3_6_1_rc2" if PLATFORM_SMP8654_SDK_3_6_1_rc2
			default "3_7_0" if PLATFORM_SMP8654_SDK_3_7_0
			default "3_8_0" if PLATFORM_SMP8654_SDK_3_8_0
			default "3_8_0" if PLATFORM_SMP8654_SDK_3_8_1
			default "3_8_0" if PLATFORM_SMP8654_SDK_3_8_2
			default "3_8_0" if PLATFORM_SMP8654_SDK_3_8_3
			default "3_8_0" if PLATFORM_SMP8654_SDK_3_8_5
			default "3_8_0" if PLATFORM_SMP8654_SDK_3_8_7
			default "3_8_0" if PLATFORM_SMP8654_SDK_3_8_8

		config SIGMA_ISDK_VERSION
			string
                        default "ISDK_3_8_5_rc_2-1"

		menu "x86 compile options"
			depends on PLATFORM_X86
		endmenu

	endmenu

	menu "Component to build"
		config BUILD_DMAOSD
			bool "build dmaosd"
